{
  "name": "nestjs-documentdb",
  "version": "0.0.1",
  "description": "NestJS project with AWS DocumentDB",
  "main": "dist/main.js",
  "scripts": {
    "start": "nest start",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register src/main.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.3",
    "@nestjs/core": "^9.4.3",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/platform-express": "^9.4.3",
    "@vendia/serverless-express": "^4.12.6",
    "express": "^4.17.1",
    "mongoose": "^6.13.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "jest": "^26.6.3",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.0.18",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  }
}
